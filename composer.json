{
    "name": "synap/ebics",
    "description": "Un projet de bibliothèque PHP pour le protocole EBICS (version française)",
    "license": "AGPL-3.0",
    "require": {
        "incenteev/composer-parameter-handler": "~2.1",
        "sabre/http": "~3.0"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ]
    },
    "extra": {
        "incenteev-parameters": {
            "file": "parameters.yml"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~4.6",
        "squizlabs/php_codesniffer": "~2.3",
        "sebastian/phpcpd": "~2.0"
    },
    "autoload": {
        "classmap": ["src/"]
    }
}
